<%= semantic_form_for @registration, url: admin_section_registrations_path(@section), html: { method: 'post' } do |form| %>
  <%= form.inputs do %>
    <%= form.input :first_name %>
    <%= form.input :last_name %>
    <%= form.input :email %>
  <% end %>
  <%= form.actions do %>
    <%= form.action :submit, label: 'Save New Student' %>
  <% end %>
<% end %>
