<%= javascript_include_tag 'application', 'prefilled_input' %>

<% if analytics? %>
  <%= render 'shared/analytics' %>
<% end %>

<%= yield :javascript %>

<% if Rails.env.test? || Rails.env.cucumber? %>
  <%= javascript_tag do %>
    jQuery.fx.off = true;
    $.ajaxSetup({ async: false });
  <% end %>
<% end %>

<%= javascript_tag do %>
  WistiaHelper.host = '<%= ENV['WISTIA_HOST'] %>'
<% end %>
