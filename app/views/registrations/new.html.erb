<% semantic_form_for @user, :url => [@course, @section, @registration] do |form| %>
  <% form.inputs do %>
    <%= form.input :first_name %>
    <%= form.input :last_name %>
    <%= form.input :email %>
  <% end %>
  <% form.buttons do %>
    <%= form.commit_button 'Save New Student' %>
  <% end %>
<% end %>
