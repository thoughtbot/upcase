<% if @coupon %>
  $("#total .price").html('<%= number_to_currency @section.calculate_price(@coupon), :precision => 0, :delimiter => '' %>');
  $(".coupon .error").hide();
  $("#total a").hide();
  $(".coupon").hide();
  $("#coupon_id").val(<%= @coupon.id %>);
<% else %>
  $(".coupon .error").show();
<% end %>
