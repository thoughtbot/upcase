<ul>
  <li>
    <%= link_to "Weekly Videos", Show.the_weekly_iteration %>
  </li>

  <li class="marketing-extra">
    <%= link_to "Forum", "https://forum.upcase.com", target: "_blank" %>
  </li>

  <li>
    <%= link_to t("shared.header.content"), "#",
      class: "table-of-contents-toggle",
      data: { role: "table-of-contents-toggle" } %>
  </li>

  <% unless team_page? %>
    <li>
      <%= link_to teams_path do %>
        <%= t("shared.header.teams") %>
      <% end %>
    </li>
  <% end %>

  <% if signed_out? && not_on_signin_path? %>
    <li>
      <% if landing_page? %>
        <%= link_to t("shared.header.sign_in"), sign_in_path %>
      <% else %>
        <%= link_to t("shared.header.sign_in"),
          sign_in_path_with_current_path_return_to %>
      <% end %>
    </li>
  <% end %>

  <li class="header-cta">
    <% if content_for?(:header_cta_link) %>
      <%= content_for(:header_cta_link) %>
    <% else %>
      <%= link_to professional_checkout_path,
        class: "site-nav__cta" do %>
        <%= t("subscriptions.join_cta") %>
      <% end %>
    <% end %>
  </li>
</ul>
